
'Actor class object
MustInherit Class Actor

	'has an id
    Protected m_id as String
	'has a (readable) name
    Protected m_name as String

    'Constructor
    Public Sub New()
    End Sub

    'The Id of this Actor
    Public ReadOnly Property Id As String
        Get
			return m_id
		End Get
    End Property

	'The (readable) name of this Actor
    Public ReadOnly Property Name As String
        Get
			return m_name
		End Get
    End Property
   

End Class
